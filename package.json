{
  "name": "headless-react-wizard",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist/", "README.md"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --out-dir dist --clean",
    "build:types": "tsc -p tsconfig.app.json --emitDeclarationOnly",
    "lint": "eslint .",
    "test": "echo \"No tests configured\" && exit 0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18 <21",
    "react-dom": ">=18 <21"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^6.7.0",
    "typescript": "~5.8.3"
  }
}
